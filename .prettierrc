{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "always",
  "jsxSingleQuote": false,
  "overrides": [
    {
      "files": "*.sql",
      "options": {
        "parser": "sql"
      }
    }
  ]
}
